{
  "name": "interview-questions",
  "version": "0.0.1",
  "description": "interview-questions",
  "main": "",
  "scripts": {
    "lint": "xo",
    "unit-test": "nyc --reporter=lcov mocha --compilers js:babel-core/register",
    "coverage": "nyc check-coverage --statement 100 --branches 100 --function 100 --lines 100",
    "test": "npm run lint && npm run unit-test --coverage && npm run coverage",
    "babel": "babel src --out-dir dist",
    "watch-babel": "nodemon --watch src --exec 'npm run babel'",
    "watch": "npm run babel && (npm run watch-babel)"
  },
  "keywords": [
    "interview questions"
  ],
  "author": "Sarbbottam Bandyopadhyay",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta2",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.10.2",
    "jsdom": "^8.0.4",
    "mocha": "^2.3.4",
    "nyc": "^6.0.0",
    "sinon": "^1.17.3",
    "xo": "^0.15.0"
  },
  "dependencies": {
    "async": "^1.5.2"
  },
  "xo": {
    "envs": [
      "mocha"
    ],
    "esnext": true,
    "space": true
  }
}
